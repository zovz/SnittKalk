{
  "manifest_version": 3,
  "name": "StudentWeb Grade Calculator",
  "version": "1.1",
  "description": "Calculates the average and lowest grade from StudentWeb (fsweb.no).",
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "host_permissions": [
    "https://fsweb.no/*"
  ],
  "icons": {
    "16": "icons/swLogo16.png",
    "48": "icons/swLogo48.png",
    "128": "icons/swLogo128.png"
  }
}